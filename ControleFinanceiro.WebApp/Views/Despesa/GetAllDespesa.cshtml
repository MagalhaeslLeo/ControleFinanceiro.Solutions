@{
    Layout = ViewBag.Layout;
}

<div class=" container ">
    <div class="row">
        <div class="col-lg-12">
            <!--begin::Card-->
            <div class="card card-custom">
                <div class="card-header flex-wrap border-0 pt-6 pb-0">
                    <h3 class="card-title">
                        Despesas
                    </h3>

                    <div class="card-toolbar">
                        <a data-target="#modalDespesa" data-toggle="tooltip" data-placement="top" data-original-title="Nova Despesa" class="showModalButton4" data-url="@Url.Action("CreateDespesa")"><i class="fas fa-plus"></i> Adicionar</a>
                    </div>

                </div>

                <div class="card-body">
                    <div class="datatable datatable-bordered" id="kt_datatable" style=""></div>
                </div>
            </div>



            <!--end::Card-->
        </div>
    </div>
</div>
@await Html.PartialAsync("_CreateDespesa", new ControleFinanceiro.Service.ServiceEntity.DespesaService())
<script src="~/js/site.js"></script>
<script src="~/js/Despesa/ListaDespesa.js"></script>

@section Scripts {

    <script src="~/js/Despesa/ListaDespesa.js"></script>

 }




